{
  "name": "mantine-vite-template",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "yarn eslint && yarn stylelint",
    "eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --ignore-pattern \"node_modules/**\" --cache",
    "stylelint": "stylelint '**/*.css' --allow-empty-input --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx,css,scss}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,css,scss}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "fix": "yarn prettier:write",
    "test": "yarn typecheck && yarn prettier:write && yarn lint && yarn build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@mantine/charts": "^7.17.3",
    "@mantine/core": "7.17.0",
    "@mantine/dates": "^7.17.1",
    "@mantine/dropzone": "^7.17.1",
    "@mantine/form": "^8.1.2",
    "@mantine/hooks": "7.17.0",
    "@mantine/notifications": "^8.1.2",
    "@react-oauth/google": "^0.12.2",
    "@tabler/icons-react": "^3.31.0",
    "@tanstack/react-query": "^5.81.5",
    "antd": "^5.26.3",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "leaflet": "^1.9.4",
    "react": "19.0.0-rc.1",
    "react-charts": "^3.0.0-beta.57",
    "react-dom": "19.0.0-rc.1",
    "react-leaflet": "5.0.0-rc.2",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^7.1.3",
    "recharts": "2"
  },
  "resolutions": {
    "@types/react": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/leaflet": "^1.9.16",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/react-resizable": "^3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.18.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.1",
    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.4.2",
    "prop-types": "^15.8.1",
    "sass-embedded": "^1.85.1",
    "storybook": "^8.5.0",
    "storybook-dark-mode": "^4.0.2",
    "stylelint": "^16.13.2",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7",
